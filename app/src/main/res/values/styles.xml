<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="Touche">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/touche</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">30sp</item>
        <item name="android:onClick">touche</item>

    </style>
    <style name="ToucheOpe" parent="Touche">

        <item name="android:background">@drawable/toucheope</item>

    </style>
    <style name="ToucheClear" parent="Touche">

        <item name="android:background">@drawable/toucheclear</item>

    </style>

</resources>
